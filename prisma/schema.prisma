generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
}

model Pokemon {
  id         Int     @id @default(autoincrement())
  name       String
  species    String
  height     Int
  weight     Int
  type       String
  rarity     Int
  captured   Boolean
  escapeCount Int    @default(0)
}